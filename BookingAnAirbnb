Scenario : Booking an Airbnb
Description : A tourist wants to book a house on Airbnb app.


Object : Consumer
	State : name, emailAddress,phoneNumber
	Behavior : visitApp(), findHouses(), placeOrderForHouse(), payMoneyToApp()
	
	
Object : App
	State : Collection of Houses
	Behavior : displayAvailableHouses(), acceptMoneyFromConsumer()
	
Object : House
	State : name, price, address
	Behavior : getPrice(), getName(),getAddress()
	
Object : Money
	state : amount	


class BookingAnAirbnb{
public static void main(String args[]){
	App airbnb = new App();

	House amli = new House();amli.name = "AMLI";amli.address = "asdfg";
	House cairns = new House();cairns.name = "CAIRNS";cairns.address = "zxcvb";
	House skye = new House();skye.name = "SKYE";skye.address = "qwert";
	House[] houses = {amli, cairns, skye};
	Consumer csm = new Consumer();
	csm.name = "CSM";csm.phoneNumber = "1234567890";csm.emailAddress="12345@gmail.com";
		
	csm.visitsApp(airbnb);
	House[] houses = airbnb.displayAvailableHouses();
	House house = csm.findTheHouseInterestedIn(houses);
	csm.placeOrderForHouse(house)
	Money money = csm.payMoneyToApp(airbnb);
	airbnb.acceptMoneyFromConsumer(csm, money);
	}

}


class Consumer{
	String name;
	String emailAddress;
	String phoneNumber;
	void visitApp(App app){}
	House findTheHouseInterestedIn(House houses[]){}
	void placeOrderForHouse(House house){}
	Money payMoneyToApp(App app){}
	
}

class App{
	House[] houses; 
	House[] displayAvailableHouses(){}
	void acceptMoneyFromConsumer(Consumer c, Money money){}
	
}

class House{
	String name;
	String address;
	Float price;
	Float getPrice(){}
	String getName(){}
	String getAddress(){}

}

class Money{
	float amount;
}

